-cc=vbccm68k -quiet -hunkdebug -sd %s -o= %s %s -O=%ld -I$VBCC/targets/m68k-amigaos/include
-ccv=vbccm68k -hunkdebug -sd %s -o= %s %s -O=%ld -I$VBCC/targets/m68k-amigaos/include
-as=vasmm68k_mot -quiet -Fhunk -nowarn=62 %s -o %s
-asv=vasmm68k_mot -Fhunk -nowarn=62 %s -o %s
-rm=rm -f %s
-rmv=rm %s
-ld=vlink -bamigahunk -x -Bstatic -Cvbcc -nostdlib -mrel $VBCC/targets/m68k-amigaos/lib/minres.o %s %s -L$VBCC/targets/m68k-amigaos/lib -lvcs -o %s
-l2=vlink -bamigahunk -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -L$VBCC/targets/m68k-amigaos/lib -o %s
-ldv=vlink -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel $VBCC/targets/m68k-amigaos/lib/minres.o %s %s -L$VBCC/targets/m68k-amigaos/lib -lvcs -o %s
-l2v=vlink -bamigahunk -t -x -Bstatic -Cvbcc -nostdlib -mrel %s %s -L$VBCC/targets/m68k-amigaos/lib -o %s
-ldnodb=-s -Rshort
-ul=-l%s
-cf=-F%s
-ml=1000
