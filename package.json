                "properties": {
                    "vasm": {
                        "type": "object",
                        "properties": {
                            "enabled": {
                                "type": "boolean",
                                "description": "If true the VASM compiler is enabled"
                            },
                            "command": {
                                "type": "string",
                                "description": "File with absolute path to the VASM compiler"
                            },
                            "args": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "title": "compiler option"
