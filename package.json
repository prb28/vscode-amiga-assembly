                            "enabled": true,
                            "command": "${config:amiga-assembly.binDir}/vasmm68k_mot",
                            "args": [
                                "-m68000",
                                "-Fhunk",
                                "-linedebug"
                            ]
                        },
                        "description": "VASM compiler configuration"
                    },
